{
  "expo": {
    "name": "YAHT - Yet Another Habit Tracker",
    "slug": "yaht",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "icon": "./assets/images/icon.png",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["fetch", "processing"]
      }
    },
    "android": {
      "package": "com.niqp.yaht",
      "permissions": ["android.permission.SCHEDULE_EXACT_ALARM", "android.permission.VIBRATE"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "build": {
        "babel": {
          "include": ["@gorhom/bottom-sheet", "react-native-reanimated"]
        }
      }
    },
    "plugins": ["./plugins/notifee-mod.js", "expo-router"],
    "scheme": "yaht-app",
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e5e2df4c-6b5a-40a0-8946-53d11fb886e2"
      }
    },
    "owner": "niqp"
  }
}
